@model List<Sample2.Models.User>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
   ViewData["Title"] = "EditUsers";
    Layout = "_Layout";
}

<h1>EditUsers</h1>

<table>
    <tr>
        <td style="font-weight:bold">Login</td>
        <td style="font-weight:bold">Email</td>
    </tr>
    @foreach (Sample2.Models.User user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>@user.Email</td>
            <td><a asp-controller="Account" asp-action="Delete" asp-route-id="@user.Id">Delete</a></td>
        </tr>
    }
</table>
